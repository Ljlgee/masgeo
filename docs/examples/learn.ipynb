    "def odd_even_number():\n",
    "    number = input(\"Enter a number, and I'll tell you if it's even or odd: \")\n",
    "    number = int(number)\n",
    "\n",
    "    if number % 2 == 0:\n",
    "        print(f\"\\nThe number {number} is even.\")\n",
    "    else:\n",
    "        print(f\"\\nThe number {number} is odd.\")\n",
    "odd_even_number()\n"
   ]
